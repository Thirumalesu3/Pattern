import java.util.*;
public class pattern1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a String");
		String a = sc.nextLine();
		int len = a.length();
		for (int i = 1; i<= len; i++) {
			for(int j = 0; j < i;j++) {
				System.out.print(a.charAt(j) + " ");
			}
			System.out.println();
		}

	}
}
